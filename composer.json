{
	"name": "pd/monolog-module",
	"description": "Integrace Monologu do projekt≈Ø PeckaDesign",
	"license": "MIT",
	"require": {
		"php": "7.4.* || 8.*",
		"monolog/monolog": "2.1.*",
		"nette/di": "2.4.* || ^3.0.0",
		"nette/application": "2.4.* || ^3.0.0",
		"nette/utils": "2.5.* || ^3.0.0",
		"tracy/tracy": "^2.6.0"
	},
	"require-dev": {
		"pd/coding-standard": "1.27.*",
		"phpstan/phpstan": "1.2.0",
		"phpstan/phpstan-strict-rules": "1.1.0",
		"nette/tester": "2.3.*",
		"nette/bootstrap": "3.0.*"
	},
	"autoload": {
		"psr-4": {
			"Pd\\MonologModule\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"PdTests\\MonologModule\\": "tests"
		}
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
